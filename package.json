{
  "name": "@sigmaott/nestjs-agenda",
  "version": "3.0.3",
  "description": "agenda for nest.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "test": "jest --forceExit",
    "lint-staged": "npx lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:SigmaOTT/nestjs-agenda.git"
  },
  "keywords": [
    "nestjs",
    "agenda"
  ],
  "author": "Alex Chen, Anh Luu",
  "license": "MIT",
  "homepage": "https://github.com/SigmaOTT/nestjs-agenda#readme",
  "dependencies": {
    "@golevelup/nestjs-common": "^1.4.2",
    "@golevelup/nestjs-discovery": "^2.3.1",
    "@golevelup/nestjs-modules": "^0.5.0",
    "@nestjs/common": "^8.0.1",
    "@nestjs/core": "^8.0.1",
    "agenda": "^4.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nestjs/testing": "8.0.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.6",
    "@types/agenda": "^4.1.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
